
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script>

$(document).ready(function(){
//样式
$("#1").css({"position":"absolute","top":"100px","left":"100px","border":"1px solid #789", "height":"200px","cursor":"move"})

	var dragging = false;
	var iX, iY;
	$("#1").mousedown(function(e) {
		dragging = true;
		iX = e.clientX - this.offsetLeft;
		iY = e.clientY - this.offsetTop;
		this.setCapture && this.setCapture();
		return false;
	});
	document.onmousemove = function(e) {
		if (dragging) {
			var e = e || window.event;
			var oX = e.clientX - iX;
			var oY = e.clientY - iY;
			$("#1").css({"left":oX + "px", "top":oY + "px"});
			return false;
		}
	};
	$(document).mouseup(function(e) {
		dragging = false;
		$("#1")[0].releaseCapture();
		e.cancelBubble = true;
	})

$("#2").css({"position":"absolute","top":"100px","left":"100px","border":"1px solid #789", "height":"200px","cursor":"move"})
	/*+++++ 拖曳效果 ++++++
	*原理：标记拖曳状态dragging，坐标位置iX、iY
	*   mousedown:fn(){dragging = true：记录起始坐标位置，设置鼠标捕获}
	*   mouseover:fn(){判断如果dragging = true，则当前坐标位置 - 记录起始坐标位置，绝对定位的元素获得差值}
	*   mouseup:fn(){dragging = false：释放鼠标捕获，防止冒泡}
	*/
	var dragging = false;
	var iX, iY;
	$("#2").mousedown(function(e) {
		dragging = true;
		iX = e.clientX - this.offsetLeft;
		iY = e.clientY - this.offsetTop;
		this.setCapture && this.setCapture();
		return false;
	});
	document.onmousemove = function(e) {
		if (dragging) {
			var e = e || window.event;
			var oX = e.clientX - iX;
			var oY = e.clientY - iY;
			$("#2").css({"left":oX + "px", "top":oY + "px"});
			return false;
		}
	};
	$(document).mouseup(function(e) {
		dragging = false;
		$("#2")[0].releaseCapture();
		e.cancelBubble = true;
	})
});

</script>

<div class="container">


  
  <div class="row">
    <div class="col-xs-12">
      <h3>User Name</h3>
      <img src="data:image/png;base64,">
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <h2>Collection</h2>
    </div>
  </div>
    <br>
  <div class="row">


        {{#each cats }}


      <div class="col-xs-2 text-center">
        <h6>{{this.name}}</h6>
        <p>House {{this.house}}</p>
        <p id={{this.id}} ><img class="image" height="200px" src="{{this.image}}"></p>
      </div>


  {{/each}}
    </div>
  </div>
</div>

